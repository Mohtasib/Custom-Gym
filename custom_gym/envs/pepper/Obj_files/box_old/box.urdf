<?xml version="0.0" ?>
<robot name="box.urdf">
  <link name="baseLink">
    <contact>
      <lateral_friction value=".7"/>
    </contact>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
       <mass value=".05"/>
       <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
				<mesh filename="box.obj" scale="0.001 0.001 0.001"/>
      </geometry>
       <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
	 	<box size="0.001 0.001 0.001"/>
      </geometry>
    </collision>
  </link>
</robot>

